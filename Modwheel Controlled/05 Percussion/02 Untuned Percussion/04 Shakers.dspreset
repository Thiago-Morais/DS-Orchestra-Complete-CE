			<sample rootNote="53" loNote="53" hiNote="53" loVel="0" hiVel="127" end="89344" path="Resources/TC - Shakers Samples/Small Swish1.wav" volume="6.020599913279624dB" />
			<sample rootNote="54" loNote="54" hiNote="54" loVel="0" hiVel="127" end="160864" path="Resources/TC - Shakers Samples/Small Swish2.wav" volume="6.020599913279624dB" />
			<sample rootNote="55" loNote="55" hiNote="55" loVel="0" hiVel="127" end="73600" path="Resources/TC - Shakers Samples/Small Swish3.wav" volume="6.020599913279624dB" />
