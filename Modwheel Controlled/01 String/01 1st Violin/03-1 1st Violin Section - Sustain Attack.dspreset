		<group name="Staccato RR 2" seqMode="random" tags="2-dynamics-ff" seqPosition="2" trigger="first" release="0.4">
			<sample rootNote="59" loNote="55" hiNote="62" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-a#3-PB.wav" volume="0.0dB" />
			<sample rootNote="65" loNote="63" hiNote="68" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-e4-PB.wav" volume="2.0dB" />
			<sample rootNote="71" loNote="69" hiNote="74" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-a#4-PB.wav" volume="2.0dB" />
			<sample rootNote="77" loNote="75" hiNote="80" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-e5-PB.wav" volume="1.0dB" />
			<sample rootNote="83" loNote="81" hiNote="86" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-a#5-PB.wav" volume="-1.0dB" />
			<sample rootNote="89" loNote="87" hiNote="92" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-e6-PB.wav" volume="0.0dB" />
			<sample rootNote="95" loNote="93" hiNote="105" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-a#6-PB.wav" volume="-1.0dB" />
		</group>
		<group name="Sustain" seqMode="random" tags="1-dynamics-ff" loopEnabled="true" loopCrossfade="6000">
			<sample rootNote="55" loNote="55" hiNote="56" loopStart="50375" loopEnd="182518" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-sus-g3.wav" start="4665" tuning="-0.10" volume="3.0dB" />
