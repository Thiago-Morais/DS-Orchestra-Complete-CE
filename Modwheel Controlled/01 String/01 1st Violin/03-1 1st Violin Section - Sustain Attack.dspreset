			<sample rootNote="64" loNote="63" hiNote="65" loopStart="56442" loopEnd="223978" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-sus-e4.wav" start="3000" tuning="-0.10" volume="3.0dB" />
			<sample rootNote="67" loNote="66" hiNote="68" loopStart="56525" loopEnd="188415" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-sus-g4.wav" start="4246" tuning="-0.10" volume="6.0dB" />
			<sample rootNote="70" loNote="69" hiNote="71" loopStart="55483" loopEnd="198154" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-sus-a#4.wav" start="2693" tuning="-0.10" volume="3.0dB" />
