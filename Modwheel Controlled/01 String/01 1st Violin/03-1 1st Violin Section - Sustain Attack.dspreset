			<sample rootNote="62" loNote="60" hiNote="65" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-c#4-PB.wav" volume="0.0dB" />
			<sample rootNote="68" loNote="66" hiNote="71" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-g4-PB.wav" volume="0.0dB" />
			<sample rootNote="74" loNote="72" hiNote="77" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-c#5-PB.wav" volume="1.0dB" />
			<sample rootNote="80" loNote="78" hiNote="83" loVel="0" hiVel="127" path="Resources/1st Violin Section Samples/1st-violins-stc-rr2-g5-PB.wav" volume="-1.0dB" />
